{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "HostOptions": {
    "ShutdownTimeout": "00:03:00"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultDB": "Data Source=webmodel.sqlite;Version=3;DateTimeKind=Utc;",
    "DefaultUpdateDB": "Data Source=webmodel-update.sqlite;Version=3;DateTimeKind=Utc;"
  },
  "GeneralSettings": {
    "PersistInterval": 600,
    "CookieSecurePolicy": "None"
  },
  "JsonOptions": {
    "MaxDepth": 64
  },
  "StartupSettings": {
            "SkipAuthentication": false ,
    "AllowPasswordBasedAuthentication": true
  },
  "Ldap": {
    "Server": "",
    "Port": 389
  },
  "_AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "",
    "ClientId": "",
    "ClientSecret": "",
    "CallbackPath": "/signin-oidc"
  },
  "DownstreamApi": {
    "BaseUrl": "https://graph.microsoft.com/v1.0",
    "Scopes": "user.read directory.read.all"
  }
}
